<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="uz.gita.quizapptest">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/test"
        android:label="@string/app_name"
        android:roundIcon="@drawable/test"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizApp"
        tools:targetApi="31">
        <activity
            android:name="uz.gita.quizapptest.TestlarActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name="uz.gita.quizapptest.MainActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="uz.gita.quizapptest.InfoActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="uz.gita.quizapptest.MenyuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                android:screenOrientation="portrait"
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>